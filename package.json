    "@types/react-map-gl": "^5.2.7",
    "babel-loader": "^8.1.0",
    "husky": "^4.3.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.1",
    "ts-loader": "^8.0.4",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "eslint": {
    "rules": {
      "jsx-a11y/accessible-emoji": "off",
      "react-hooks/exhaustive-deps": "off",
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error",
            "debug"
          ]
        }
      ]
    }
  },
  "resolutions": {
    "@turf/difference": "6.0.1"
  },
  "filesize": {
    "track": [
      "./dist/*.production.min.js"
    ]
  }
