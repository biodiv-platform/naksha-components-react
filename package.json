    "immutable": "^4.0.0-rc.12",
    "react-gbus": "^1.1.2",
    "react-highlighter": "^0.4.3",
    "react-map-gl": "^5.2.8",
    "react-map-gl-draw": "^0.20.1",
    "react-tiny-virtual-list": "^2.2.0",
    "react-virtualized-auto-sizer": "^1.0.2",
