{
  "name": "naksha-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "repository": "strandls/naksha-components-react",
  "author": "harshzalavadiya",
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint:naksha-gmaps-draw": "cd packages/naksha-gmaps-draw && eslint .",
    "lint:naksha-components-react": "cd packages/naksha-components-react && eslint .",
    "lint:naksha-gmaps-view": "cd packages/naksha-gmaps-view && eslint .",
    "lint:naksha-mapbox-draw": "cd packages/naksha-mapbox-draw && eslint .",
    "lint:naksha-mapbox-list": "cd packages/naksha-mapbox-list && eslint .",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules dist",
    "prerelease": "yarn run build",
    "release": "lerna publish --no-private",
    "release:local": "lerna publish --no-git-tag-version --force-publish --no-private --no-push --registry=\"http://localhost:4873/\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.17",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lerna": "^5.1.4"
  }
}
